{
    "manifest_version": 3,
    "name": "YouTube Search Results",
    "version": "1.0",
    "permissions": ["activeTab", "tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/icon16.png"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png"
    }
}